{
  "name": "prisma-rls",
  "version": "0.5.0",
  "description": "Prisma client extension for row-level security on any database",
  "license": "MIT",
  "keywords": [
    "database",
    "prisma",
    "prisma-client",
    "prisma-extension",
    "rls",
    "row-level-security"
  ],
  "author": "s1owjke",
  "homepage": "https://github.com/s1owjke/prisma-rls",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:watch": "rm -rf dist && tsc --watch",
    "lint": "yarn biome check",
    "lint:fix": "yarn biome check --write",
    "typecheck": "tsc --noEmit",
    "test": "jest"
  },
  "peerDependencies": {
    "@prisma/client": ">=4.16.1"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.2",
    "@prisma/client": "^5.22.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "jest": "^29.7.0",
    "prisma": "^5.22.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "prisma": {
    "schema": "db/schema.prisma",
    "seed": "ts-node db/seed.ts"
  }
}
